<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<VBox xmlns:fx="http://javafx.com/fxml"
      xmlns="http://javafx.com/javafx"
      styleClass="green-theme"
      fx:controller="com.canyue.mqtt.ui.controller.ClientController"
      maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
    <stylesheets>
        <URL value="@/css/main.css"/>
    </stylesheets>
    <children>
        <fx:include source="conn.fxml" fx:id="conn"/>
        <TabPane id="tabPane" fx:id="tabPane" disable="true" focusTraversable="false" maxHeight="1.7976931348623157E308"
                 maxWidth="1.7976931348623157E308" prefHeight="616.0" prefWidth="820.0" tabClosingPolicy="UNAVAILABLE"
                 tabMaxHeight="30.0" tabMaxWidth="100.0" tabMinHeight="30.0" tabMinWidth="100.0" VBox.vgrow="ALWAYS">
            <tabs>
                <fx:include source="pub.fxml" fx:id="pub"/>
                <fx:include source="sub.fxml" fx:id="sub"/>
                <fx:include source="his.fxml" fx:id="his"/>
            </tabs>
        </TabPane>
    </children>
</VBox>
